{
    "name": "yangzx/laravel-invite-code",
    "description": "laravel invite code pkg",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "torann/hashids": "^3.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "classmap": [
            "src/Libs/HashGenerator.php",
            "src/Libs/Hashids.php"
